    </DataTemplate>

    <DataTemplate DataType="{x:Type switchview:SwitchViewUpdateParameter}">
        <view:UpdateParameterUC></view:UpdateParameterUC>
    </DataTemplate>

    <DataTemplate DataType="{x:Type switchview:SwitchViewRepairReceipt}">
        <view:RepairReceiptUC></view:RepairReceiptUC>
    </DataTemplate>

    <DataTemplate DataType="{x:Type switchview:SwitchViewBill}">
        <view:BillUC></view:BillUC>
    </DataTemplate>

    <DataTemplate DataType="{x:Type switchview:SwitchViewShowWageAndMaterial}">
        <view:ShowWageAndMaterialUC></view:ShowWageAndMaterialUC>
    </DataTemplate>

    <DataTemplate DataType="{x:Type switchview:SwitchViewShowAccessoriesList}">
        <view:ShowAccessoriesListUC></view:ShowAccessoriesListUC>
    </DataTemplate>

    <DataTemplate DataType="{x:Type switchview:SwitchViewShowWagesList}">
        <view:ShowWagesListUC></view:ShowWagesListUC>
    </DataTemplate>

    <DataTemplate DataType="{x:Type switchview:SwitchViewShowCarBrandsList}">
        <view:ShowCarBrandsListUC></view:ShowCarBrandsListUC>
    </DataTemplate>
</ResourceDictionary>